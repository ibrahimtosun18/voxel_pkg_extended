<launch>
  <node name="mls_upsampler_node" pkg="voxel_pkg" type="mls_upsampler_node" output="screen">
    <param name="input_pcd_file" value="$(find voxel_pkg)/data/251371071.pcd" />
    <param name="output_pcd_file" value="$(find voxel_pkg)/data/upsampled_pcd.pcd" />

    <!-- Set the search radius according to your needs -->
    <param name="search_radius" value="0.3" />
    <param name="/use_sim_time" value="true" />

  </node>
</launch>
